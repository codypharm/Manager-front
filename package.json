{
  "name": "Managerfront",
  "version": "1.0.0",
  "description": "A business management system",
  "main": "app/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder --ia32 --x64 -w"
  },
  "build": {
    "appId": "managerfront",
    "productName": "Managerfront",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "uninstallDisplayName": "Managerfront uninstaller",
      "license": "license.md",
      "allowToChangeInstallationDirectory": false,
      "deleteAppDataOnUninstall": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techyNonso/vemon.git"
  },
  "author": "Ikeji Chukwunonso",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/techyNonso/vemon/issues"
  },
  "homepage": "https://github.com/techyNonso/vemon#readme",
  "devDependencies": {
    "electron": "^7.3.3",
    "electron-builder": "^22.13.1",
    "eslint": "^6.8.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "install-peers": "^1.0.3",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "croppie": "^2.6.4",
    "dotenv": "^10.0.0",
    "electron-fetch": "^1.4.0",
    "electron-handlebars": "^2.0.0",
    "electron-less": "^1.1.0",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.9",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "jsbarcode": "^3.11.5",
    "moment": "^2.25.3",
    "nigerian-phone-number-validator": "^1.0.0",
    "node-couchdb": "^1.3.0",
    "notyf": "^3.10.0",
    "npm-install-peers": "^1.2.2",
    "popper.js": "^1.16.1",
    "print-js": "^1.6.0"
  }
}
